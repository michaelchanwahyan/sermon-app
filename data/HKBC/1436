	var fontColor = $("a.article-citation").css("color");
	if ($("a.article-citation").length > 0){
		$(".btn-show-scripture").show();
		
		$("a.article-citation").first()
			.click()
			.animate({
				"background-color": "#c90000",
				"color": "#FFFFFF",
			}, 300)
			.delay(100)
			.animate({
